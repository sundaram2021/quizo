generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("NEXT_PUBLIC_DATABASE_URL")
}

model users {
  id       Int    @id @default(autoincrement())
  name     String
  email    String @unique(map: "users_email_unique") @db.VarChar
  password String @db.VarChar
}
